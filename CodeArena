#include <iostream>

using namespace std;

int main()
{
    int t, r,g,b, count_r=0, count_g=0, count_b=0;
    cin>>t>>r>>g>>b;
    int color[t][3];
    int red=0, green=0, blue=0, yellow=0, cyan=0, magenta=0, white=0, black=0;
    for(int r_i=0; r_i<t;)
    {   
    	if(count_r==0){
        while(count_r<r && r_i<t){
         color[r_i++][0]=0;
         count_r++;
        }
    	}
    	else{
    		while(count_r>0 && r_i<t)
        {
            color[r_i++][0]=1;
            count_r--;
        }
    	}
    }
    for(int g_i=0; g_i<t;)
    {   
        if(count_g==0){
        while(count_g<g && g_i<t){
         color[g_i++][1]=0;
         count_g++;
        }
    	}
    	else{
    		while(count_g>0 && g_i<t)
        {
            color[g_i++][1]=1;
            count_g--;
        }
    	}
    }
    for(int b_i=0; b_i<t;)
    {   
       if(count_b==0){
        while(count_b<b && b_i<t){
         color[b_i++][2]=0;
         count_b++;
        }
    	}
    	else{
    		while(count_b>0 && b_i<t)
        {
            color[b_i++][2]=1;
            count_b--;
        }
    	}
    }
    for(int t_i=0; t_i<t; t_i++)
    {
        int value= (color[t_i][0]*4)+(color[t_i][1]*2)+(color[t_i][2]*1);
        switch(value)
        {
            case 0: black++;break;
            case 1: blue++;break;
            case 2: green++;break;
            case 3: cyan++;break;
            case 4: red++;break;
            case 5: magenta++;break;
            case 6: yellow++;break;
            case 7: white++;break;
        }
    }
    cout<<red<<" "<<green<<" "<<blue<<" "<<yellow<<" "<<cyan<<" "<<magenta<<" "<<white<<" "<<black;
    return 0;
}
